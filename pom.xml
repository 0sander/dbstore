<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.cinefms.dbstore</groupId>
	<artifactId>dbstore</artifactId>
	<packaging>pom</packaging>
	<modules>
		<module>dbstore-api</module>
		<module>dbstore-cache-api</module>
		<module>dbstore-cache-memcached</module>
		<module>dbstore-redis</module>
		<module>dbstore-mongo</module>
		<module>dbstore-cache-ehcache</module>
	</modules>
	<properties>
		<junit.version>4.11</junit.version>
		<mockito.version>1.9.5</mockito.version>
		<jackson.version>2.2.3</jackson.version>
		<spymemcached.version>2.11.4</spymemcached.version>
		<redisson.version>1.1.5</redisson.version>
	</properties>
	<licenses>
		<license>
			<name>MIT License</name>
			<url>http://www.opensource.org/licenses/mit-license.php</url>
		</license>
	</licenses>
	<developers>
		<developer>
			<name>Ruben Malchow</name>
			<email>ruben@cinefms.com</email>
			<organization>CineFMS</organization>
			<organizationUrl>http://www.cinefms.com</organizationUrl>
		</developer>
	</developers>
	<scm>
		<connection>scm:git:https://github.com/rmalchow/dbstore</connection>
		<developerConnection>scm:git:https://github.com/rmalchow/dbstore</developerConnection>
		<url>https://github.com/rmalchow/spring-api-tester</url>
	</scm>
	<distributionManagement>
		<repository>
			<id>apitester-release</id>
			<url>https://infra.cinefms.com/nexus/content/repositories/apitester-release/</url>
		</repository>
		<snapshotRepository>
			<id>apitester-snapshot</id>
			<url>https://infra.cinefms.com/nexus/content/repositories/apitester-snapshot/</url>
		</snapshotRepository>
	</distributionManagement>


